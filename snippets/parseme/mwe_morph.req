% Search for a MWE with some morphological restrictions

pattern {
  MWE [label];                                          % Search for a MWE node…
  MWE -[parseme=MWE]-> X1; X1 [upos=VERB];              % …which contains a verbal token …
  MWE -[parseme=MWE]-> X2; X2 [upos=NOUN, Number=Plur]; % …and a plural noun token."
}
